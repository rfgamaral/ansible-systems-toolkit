{
  "logging": {
    "append": true
  },
  "cache": {
    "path": "$PTS_CACHE_DIR/.cache/trakt"
  },
  "sync": {
    "trakt_to_plex": {
      "watched_status": true,
      "ratings": true,
      "liked_lists": false,
      "watchlist": false
    },
    "plex_to_trakt": {
      "watched_status": true,
      "ratings": false,
      "collection": true
    }
  },
  "watch": {
    "scrobble_threshold": 80,
    "add_collection": true,
    "remove_collection": true,
    "username_filter": true
  },
  "xbmc-providers": {
    "movies": "imdb",
    "shows": "tvdb"
  },
  "excluded-libraries": [
    "Samples"
  ]
}
