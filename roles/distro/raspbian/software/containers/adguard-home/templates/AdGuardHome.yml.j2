  server_name: adguard.{{ config_network_domain }}
  force_https: false
  port_https: 443
  port_dns_over_tls: 853
  port_dns_over_quic: 784
  port_dnscrypt: 0
  dnscrypt_config_file: ''
  allow_unencrypted_doh: true
  certificate_chain: ''
