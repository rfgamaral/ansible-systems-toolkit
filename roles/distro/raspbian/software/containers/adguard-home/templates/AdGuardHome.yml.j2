block_auth_min: 15
http_proxy: ''
language: en
theme: auto
debug_pprof: false
web_session_ttl: 720
dns:
  bind_hosts:
    - 0.0.0.0
  port: 53
  anonymize_client_ip: false
  protection_enabled: true
  blocking_mode: default
  blocking_ipv4: ''
  blocking_ipv6: ''
  blocked_response_ttl: 10
  protection_disabled_until: null
