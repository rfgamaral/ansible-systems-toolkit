{
  "log_debug_messages": false,
  "logging": {
    "append": false
  },
  "cache": {
    "path": "$PTS_CACHE_DIR/.cache/trakt"
  },
  "sync": {
    "trakt_to_plex": {
      "watched_status": true,
      "ratings": true,
      "liked_lists": false,
      "watchlist": false
    },
    "plex_to_trakt": {
      "watched_status": true,
      "ratings": false,
      "collection": false
    }
  },
  "watch": {
    "username_filter": true
  },
  "xbmc-providers": {
    "movies": "imdb",
    "shows": "tvdb"
  },
  "excluded-libraries": [
    "Samples"
  ]
}
